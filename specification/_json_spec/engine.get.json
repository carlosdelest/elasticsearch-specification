{
  "engine.get": {
    "documentation": {
      "url": "http://todo.com/tbd",
      "description": "Returns the details about an engine."
    },
    "stability": "experimental",
    "visibility": "feature_flag",
    "feature_flag": "xpack.ent-search.enabled",
    "headers": {
      "accept": [
        "application/json"
      ],
      "content_type": [
        "application/json"
      ]
    },
    "url": {
      "paths": [
        {
          "path": "/_engine/{engine_id}",
          "methods": [
            "GET"
          ],
          "parts": {
            "engine_id": {
              "type": "string",
              "description": "The name of the engine"
            }
          }
        }
      ]
    }
  }
}
